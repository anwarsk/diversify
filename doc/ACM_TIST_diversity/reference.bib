  title = {Gait Recognition Using Radon Transform and Linear Discriminant Analysis},
  journal = {IEEE Transactions on Image Processing},
  year = {2007},
  volume = {16},
  pages = {731--740},
  number = {3},
  month = {March},
  annote = {This paper attempted to investigate the problem of feature extraction
	for gait representation and recognition. A new system was proposed
	based on the Radon transform of binary silhouettes. Specifically,
	a template was computated from the transformed silhouettes for each
	gait sequence. Then, the set of templates was then processed by linear
	discriminant analysis and subspace projection for compact representation
	of gait sequence. In this way, each gait sequence was characteristic
	of a low-dimensional feature vector, which only consisted of 20-70
	Radon template coefficients. Regarding gait recognition, a test feature
	vector was compared to feature vectors in a reference gait database
	using Euclidean distance. Experiments conducted on USF's Gait Challenge
	database demonstrated that the proposed system was significantly
	superior to the state-of-the-art methods for gait recognition such
	as the MIT method, the CMU method, the USF baseline method, and the
	UCR method.},
  keywords = {Gait and linear discriminant analysis (LDA) and random transform and
	recognition}
}

