@INPROCEEDINGS{liu0791,
  author = {Shu-Chang Liu and Jing Bi and Zhi-Qiang Jia and Rui Chen and Jie
	Chen and Min-Min Zhou},
  title = {Automatic Audio Classification and Speaker Identification for Video
	Content Analysis},
  booktitle = {The Proceedings of The Eighth ACIS International Conference on Software
	Engineering, Artificial Intelligence, Network, and Parrallel/ Distributed
	Computing},
  year = {2007},
  volume = {2},
  pages = {91--96},
  annote = {This paper studied the problem of applying audio content analysis
	techniques in content-based video parsing. The authors proposed a
	scheme, which utilized audio classification and speaker identification
	techniques for video content analysis. Specifically, the scheme included
	three steps. First, it extracted soundtrack (PCM data) from video
	stream and partitioned the soundtrack into homogeneous segments by
	using rule and support vector machine (SVM) based classifier. The
	rule-based classifier separated an audio clip into silences and nonsilence
	ones. Then for these non-silence sub-clips, SVMs are employed to
	classify them into three classes: speech, music and environmental
	sound. Furthermore, the nonsilence sub-clip was classified into speech
	and non-speech classes, and then the non-speech was further classified
	into music and environmental sound. Second, based on spectral clustering,
	the fixed-length speech clips (for example 2s) randomly selected
	from speech segments were clustered into several clusters. Speech
	clustering was based on the analysis of a similarity matrix. Kullback-Leibler
	Divergence was used as the similiarity measure. Here, the clustered
	speech feature datasets initialize and train GMMs for each speaker.
	Finally, speaker identification was performed based on the trained
	GMMs. The audio features used in the scheme consisted of short-time
	energy, zero-crossing rate, sub-band energy, sub-band energy ratio,
	and Mel-frequency cepstral coefficients (MFCC). Experimental testing
	system was builted on AFsp, libmad, HTK, libSVM, matrix and Torch.
	Experimental evaluations on some video clips demonstrated the best
	accuracy of the schme was about 65 percent.}
}

