@ARTICLE{maurer08821,
  author = {Donald E. Maurer and John P. Baker},
  title = {Fusing multimodal biometrics with quality estimates via a Bayesian
	belief network},
  journal = {Pattern Recognition},
  year = {2008},
  volume = {41},
  pages = {821--832},
  number = {3},
  month = {March},
  annote = {This paper investigated the problem of fusing multimodal biometrics.
	Traditionally, fusion methods included processing biometric modalities
	sequentially until an acceptable match was obtained, utilizing logical
	(AND/OR) operations, summing similarity scores, and combining scores
	from separate classifiers fro each modality. In this paper, the author
	proposed a novel fusion architecture based on Bayesian belief networks
	(BBN). This approach exploited the graphical structure of Bayes nets
	to define and model statistical dependencies between relevant variables,
	namely, per sample measurements, such as match scores and corresponding
	quality estimates, and global decision variables. These statistical
	dependencies were in the form of conditional distributions, which
	were modeled as Gaussian, gamma, log-normal or beta distribution.
	Each of these distrubtions was decided by its mean and variance,
	thus the selected models reduced significantly the training data
	requirements. The proposed method has been used to fuse fingerprints
	and voice on the fingerprint data from the NIST, and unpublished
	government-owned database of fingerprints collected during field
	operation. The employment of BBN for information fusion was first
	proposed in this paper. Based on the description in this paper, I
	need to further investigate BBN to study the possibility of application
	in my project.},
  keywords = {Biometrics and Multimodal biometrics and Bayesian belief networks
	and Fingerprint match scores and Voice match scores and Information
	fusion}
}

