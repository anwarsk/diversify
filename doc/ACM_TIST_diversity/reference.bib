@INPROCEEDINGS{chen06781,
  author = {Lei Chen and Sule G\"{u}nd\"{u}z},
  title = {Mixed Type Audio Classification with Support Vector Machine},
  booktitle = {The Proceedings of 2006 IEEE International Conference on Multimedia
	and Expo},
  year = {2006},
  pages = {781--784},
  month = {July},
  annote = {This paper investigated the problem of classification of mixed type
	audio data, such as clips having speech with music as background.
	Based on Support Vector Machine (SVM), the authors proposed a mixed
	type audio classification system. The features used in this system
	can be categorized into two types, namely, audio features and four
	different representation formats for each feature. The audio features
	consisted of variance of zero crossing rate and silence ratio from
	time domain, harmonic ratio and sub-band energy from frequency domain.
	The extracted audio data were first segmented into 5 second audio
	clips and each clip was further divided by a 1 second window into
	5 segments, from which the four audio features were extracted. Finally,
	for each clips, four different representation format as final features
	were calculated in terms of mean, max, min and (max+min)/2. This
	system was capable of classifying audio data into five types: music,
	speech, environment sound, speech mixed with music, and music mixed
	with environment sound. The experiments were conducted on the audio
	clips extracted from movies. Experimental results demonstrated that
	the proposed system was superior to k Nearest Neighbor (k-NN), Neural
	Network (NN), and Naive Bayes (NB). Future work includes the division
	of environment sound into finer subclasses, such as nature environment
	sound, animal sound and man-made sound. Furthermore, a classification
	using both labelled and unlabelled data as training set for SVM is
	expected to be developed due to the expensive expenditure of manually
	labelling of audio data.}
}

