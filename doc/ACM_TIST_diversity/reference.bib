@INPROCEEDINGS{chakrabarti08ContextualAdvertising,
  author = {Deepayan Chakrabarti and Deepak Agarwal and Vanja Josifovski},
  title = {Contextual advertising by combining relevance with click feedback},
  booktitle = {Proceeding of the 17th International Conference on World Wide Web},
