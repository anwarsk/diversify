@ARTICLE{Holloway03QuestionSelectionforMultiAttributeDecisionAiding,
  author = {Hillary A. Holloway and Chelsea C. White III},
  title = {Decision Aiding: Question selection for multi-attribute decision-aiding},
  journal = {European Journal of Operational Research},
