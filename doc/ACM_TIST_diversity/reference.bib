@INPROCEEDINGS{klapuri013381,
  author = {Anssi P. Klapuri},
  title = {Multipitch estimation and sound separation by the spectral smoothness
	principle},
  booktitle = {Proceedings of IEEE International Conference on Acoustics, Speech,
	and Signal Processing},
  year = {2001},
  volume = {5},
  pages = {3381--3384},
  annote = {This paper presents the spectral smoothness principle with its application
	in multipitch estimation and sound separation. Here, it is assumed
	that the spectral envelopes of real sound sources tend to be continuous.
	This papers presents evidence for the importance of spectral smoothness
	both in human and computational MPE, and three algorithms to implement
	this principle. The aim of the spectral smoothness algorithm is to
	use the pitch information to produce a more appropriate estimate
	for the spectrum of a separated sound before it is subtracted from
	the mixture. Experiments were conducted using an experimental model,
	where the spectral smoothness was either used in different ways,
	or was completely ignored. The data set comprises sung vowels and
	the whole pitch range of 26 musical instruments. By including spectral
	smoothness, the pitch error rate in random four-voice mixture dropped
	from 18 percent to 8.1 percent, and in musical four-voice mixtures
	from 25 percent to 12 percent. It indicates that it is useful to
	include spectral smoothness for MPE. From the signal seperation point
	of view, the audio events detection problem can be described as follows:
	Given an audio clip, our aim is to seperate the single audio event
	of interest. One method to seperate single audio events from a mixture
	of multiple streams is as follows: First, it is to find the pitch
	of the most prominent sound in the interference of other harmonic
	and noisy sounds. Second, estimate the precise frequencies and amplitudes
	of the harmonics partials of the prominent sound. Third, the spectrum
	of the detected sound is linearly subtracted from the mixture. These
	operations are then repeated for the residual signals. Now, suppose
	we have separated and got three of the most prominent sound events,
	the problem of audio events detection is to decide whether the separated
	sound events is those of interested. However, the method faces difficulties
	in three circumstances, 1) multi-speakers, 2) multi-notes, 3) multi-instruments,
	and 4) inharmonics of unvoiced speech. Although we can first assume
	that the component of mixture is from single speaker, single note,
	and single instruments, this framework is difficult to be extended
	for handling the three cased mentioned above.}
}

