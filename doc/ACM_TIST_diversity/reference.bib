@ARTICLE{chu06185,
  author = {Wei Chu and Beno\^{\i}t Champagne},
  title = {A simplified early auditory model with application in audio classification},
  journal = {Canadian Journal of Electrical and Computer Engineering},
  year = {2006},
  volume = {31},
  pages = {185--189},
  number = {4},
  month = {Fall},
  annote = {This paper investigated the effect of background noise on classification
	performance for audio classification and segmentation. Recently,
	there is an early auditory model, which calculated a so-called auditory
	spectrum and proved to be robust in a noisy environment. However,
	this model was characteristic of high computational requirements
	and the use of nonlinear processing. This paper proposed certain
	modifications to develop a simplified version of this model, which
	is linear except for the computation of the square-root value of
	the energy. Experiments of speech/music and speech/non-speech classification
	tasks were used to evaluate the proposed system. Experimental results
	demonstrated that both the original auditory spectrum and the proposed
	simplified auditory spectrum achieved more robust performance in
	noisy test cases than the conventional fast Fourier transformed-based
	spectrum. In addition, desipte of a reduced computational complexity,
	the performance of the proposed simplified auditory spectrum was
	close to that of the original auditory spectrum.
	
	 Revised annotated bibliography: Chu et al. presented a simplified
	version of this model since it was characteristic of high computational
	requirements and the use of nonlinear processing. The proposed model
	is linear except the for the computation of the square-root value
	of the energy. From the original model and the simplified model,
	the authors extracted the auditory spectrum. As for speech/music
	(speech/nonspeech) classification under noisy environment, the average
	classification error rate was 5.6\% (4.0\%) and 7.3\% (4.9\%), which
	was significantly lower than that based on auditory spectrum based
	on FFT with an average error rate of 29.2\% (21.4\%).}
}

