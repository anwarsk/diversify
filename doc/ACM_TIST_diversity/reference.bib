@INPROCEEDINGS{nielsen06Pitch,
  author = {Andreas B. Nielsen and lars K. Hansen and Ulrik Kjems},
  title = {Pitch based sound classification},
  booktitle = {Proceedings of IEEE International Conference on Acoustics, Speech
	and Signal Processing},
  year = {2006},
  volume = {3},
  month = {May},
  annote = {This paper studies the problem of sound classification based solely
	on the pitch. The authors presents a method consisting three parts:
	a pitch estimator, pitch features, and a classification model. The
	pitch estimation is conducted using harmonic product spectrum (HPS).
	28 features are extracted from the estimated pitch and pitch reliability.
	Pitch reliability is the value by dividing the total energy in the
	HPS with the maximum peak in HPS. Four of the 28 features described
	in details are \begin{itemize} \item Standard deviation of pitch
	reliability: standard deviation of pitch reliability shows good separation
	of all classes with speech having high values, noise low and music
	in between. \item Difference between subsequent pitch samples: difference
	between subsequent pitch samples describes the dynamical property
	of the pitch. It is good for separating music from speech and noise,
	because music has many constant pitch parts and therefore a high
	pitch difference value. \item Tone distance: tone distance is the
	distance from the pitch of a 12'th octave musical note. It is good
	for separating the music from speech and noise. \item Pitch change:
	pitch change feature measures the difference between the highest
	and the lowest pitch in a reliable window. \end{itemize} The classification
	model is based on the soft-max output function due to its simplicity.
	This paper motivates to use statistical representation of pitch.
	The representation may be employed so that it can be used for my
	project. In addition, I will pay attention to develop new representation
	for audio events detection.}
}

