@CONFERENCE{Aleksic03,
  author = {Petar S. Aleksic and Aggelos K. Katsaggelos},
  title = {An audio-visual person identification and verification system using
	{F}{A}{P}s as visual features},
  booktitle = {Workshop on Multimedia User Authentication},
  year = {2003},
  pages = {80--84},
  address = {Santa Barbara, California, USA},
  month = {December},
  annote = {This paper discussed the problem of person identification and verification.
	To improve the performance of traditional automatic speaker recognition
	system, the authors proposed a novel approach utilizing the inegration
	of visual stream with audio stream. The Facial Animation Parameters
	(FAPs), supported by the MPEG-4 standard, were used as the visual
	features; the Mel-Frequency Cepstral Coefficients (MFCC), signal
	energy with their first and second order time derivatives were employed
	as the audio features. The feature vectors for visual and audio were
	of 9 and 39 dimensions, respectively. The observation vectors was
	formed by appending the visual observation feature vectors to the
	audio observation feature vectors, which were modelled by Hidden
	Markov Models (HMMs). The HMMs for each speaker is left-right topology
	with 10 states. Based on the proposed method, the authors presented
	two frameworks of speaker identification and verification, and both
	of them were tested using the Carnegie Melon University (CMU) audio-visual
	database. A Gaussian noise source ranging from 0dB to 20dB was used
	to test performance under noisy conditions. Under clean condition,
	20dB, 10dB, and 0dB, the person identification error (percent) was
	5.13, 7.69, 10.26 and 12.82, respectively. For the person verification
	results, the person verification results judged by false accept rate,
	false reject rate and equal error rate were better than that using
	audio only. The result indicated that the proposed system can considerately
	improve the performance of speaker recognition based only on audio.\\
	Since the database employed for evaluating the proposed method contains
	only 10 subjects, the results cannot be considered reliable. To further
	support the conclusion, larger database should be used. }
}

