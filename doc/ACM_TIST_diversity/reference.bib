@INPROCEEDINGS{abe962646,
  author = {Mototsugu Abe and Shigeru Ando},
  title = {Application of loudness/pitch/timbre decomposition operators to auditory
	scene analysis},
  booktitle = {Proceedings of IEEE International Conference on Acoustics, Speech,
	and Signal Processing},
  year = {1996},
  volume = {5},
  pages = {2646--2649},
  annote = {This paper studies the problem of describe and pursue the individual
	coherency of sound streams in wavelet domain. The authors extend
	the method based on nonlinear operators for decomposing a change
	energy of sound into loudness, pitch, and timbre. In addition, the
	authors assume that a change of sound from single source is coherent
	almost everywhere. By using Parzen's non- parametric estimation and
	Kalman filtering of loudness change rate and pitch shift rate, the
	authors conducted experiments to extract the most salient stream
	from multiple sound streams. The idea in this paper is that a single
	source from an auditory scene is a plane in the time-frequency gradient
	space (TFGS). Furthermore, for non-overlapping sound streams from
	different sources, TFGS distribution still has coherency such that
	it is a superposition of individual planes.}
}

