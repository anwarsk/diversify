@INPROCEEDINGS{goldberger051985,
  author = {Jacob Goldberger and Hagai Aronowitz},
  title = {A Distance Measure Between GMMs Based on the Unscented Transform
	and its Application to Speaker Recognition},
  booktitle = {The Proceedings of Interspeech'2005-Eurospeech},
  year = {2005},
  pages = {1985--1988},
  address = {Lisbon, Portugal},
  month = {September},
  annote = {The problem of computing a distance measure between two GMMs learned
	from speech segments is studied in this paper. Kullback-Leibler divergence
	is a widely used measure between two distributions. However, there
	is no closed form expression for the KL-divergence betwen two GMMs.
	Based on the unscented transform, Jaco et al. proposed an accurate
	and efficiently computed approximation of the KL-divergence. The
	proposed method was applied for speaker recognition. The acoustic
	features are 13-dimension MFCCs with the first time derivatives.
	UBM was trained based on the SPIDRE corpus, which consists of conversational
	speech. NIST-2004 speaker evaluation set was used for evalutaion
	of the proposed dissimilarity measure. The data set used for training
	consisted of 616 1-sided single conversations for training 616 target
	models, 1174 1-sided test converstions were used for testing. 3 testing
	conditions were used to evaluated, namely, full conversations modeled
	by 2048 Gaussians, 30 second speech segments modeled by 128 Gaussianms,
	and 3 second speech segments model modeled by 128 Gaussians. The
	proposed method has been compared with the matching-based algorithms.
	Under three cases, the propose method outperformed the matching-based
	method by 0 percent (13.2 percent VS. 13.2 percent), 0.6 percent
	(15.8 percent VS. 15.2 percent), and 0.2 percent (17.8 percent VS.
	17.6 percent) for EER.}
}

@INPROCEEDINGS{goldberger071,
  author = {Jacob Goldberger and Hayit Greenspan and Jeremie Dreyfuss},
  title = {An Optimal Reduced Representation of a MoG with Applicatios to Medical
	Image Database Classification},
  booktitle = {The Proceedings of the IEEE Conference on Computer Vision and Pattern
	Recognition},
  year = {2007},
  pages = {1--6},
  month = {June},
  annote = {The problem of image database classification has been studied. Jacob
	et al. proposed a general framework for image classification. This
	framework is comprised of a continuous and probabilistic image representation
	scheme using Gaussian mixture modeling (MoG) along with information-theoretic
	image matching measures (KL). Firstly, a large Gaussian mixture models
	was constructed based on all the images in the category. Then, a
	category model was derived by learning a reduced model from the original.
	This operation was based on Unscented-Transform. Simulation experiments
	and categorization of a real medical image database have been conducted
	to validate the proposed framework. Experimental results indicated
	the higher performance of the proposed method.}
}

