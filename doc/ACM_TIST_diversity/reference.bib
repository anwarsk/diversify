@ARTICLE{kam9952,
  author = {Moshe Kam and Chris Rorres and Wei Chang and Xiaoxun Zhu},
  title = {Performance and geometric interpretation for decision fusion with
	memory},
  journal = {IEEE Transactions on Systems, Man, and Cybernetics--Part A: Systems
	and Humans},
  year = {1999},
  volume = {29},
  pages = {52--62},
  number = {1},
  month = {January},
  annote = {In this paper, the problem of decision fusion was discussed. Decision
	fusion is one of the most important module for parallel and serial
	decentralized distributed detection systems. Previous research involved
	in parallel architecture assumes memoryless local decision makers
	(LDM's) and a memoryless data fusion center (DFC). However, it is
	possible for components to remember and utilize past decisions for
	synthesis of new decisions, which may potentially yield performance
	improvement. This paper focused on the decision fusion of a binary
	distributed system, which comprised a bank of LDM's and DFC. Here,
	all LDM's survey a common volume for a binary phenomenon. Each LDM
	reached a binary decision. Then, the decisions were transmitted to
	the DFA through noiseless communication channels. Next, the DFA optimally
	combined the local dicision to obtain a global decision, which minimized
	a Bayesian objective function. A new architecture for decision fusion
	was then proposed, which synthesized each new decision based on both
	the local decisions and the most recent decision of DFC. Furthermore,
	the authors demonsrated that the proposed architecture converged
	to a steady-state decision in finite time with probability one, in
	a stationary environment.}
}

