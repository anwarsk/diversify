@INBOOK{yang07930,
  pages = {930--937},
  title = {Manifold Analysis in Reconstructed State Space for Nonlinear Signal
	Classification},
  publisher = {Springer Berlin},
  year = {2007},
  editor = {De-Shuang Huang and L. Heutte and M. Loog},
  author = {Su Yang and I-Fan Shen},
  series = {Lecture Notes in Computer Science},
  address = {Heidelberg},
  month = {July},
  annote = {This paper studise the problem of feature extraction for nonlinear
	signal classification. A framework based on manifold learning in
	reconstructed state space is proposed for feature extraction due
	to 1) manifolds are of importance in characterizing chaotic attractorss
	and 2) there are a large number of toolkits in the context of manifold
	learning. This paper serves to bridge the gap between nonlinear science
	and manifold learning. Specifically, the proposed method works according
	to the following steps. First, the time series of interest is embedded
	into a high-dimensional space via state space reconstruction. Next,
	locally linear embedding (LLE) is employed to obtain the local manifold
	characteristics around every point in the reconstructed state space.
	Finally, all the local features are summarised into a global representation
	via principal component analysis (PCA). The proposed framework is
	applied for two cases of oceanic and EEG signal classification. As
	for the classification of oceanic siganl, the proposed method yeilds
	an accuracy of 97.93 percent, compared with that based on Lyapunov
	and spectra, 97.73 percent and 97.17 percent, respectively. For classification
	of EEG signal, the accurcay of the proposed method is 95.4 percent,
	compared with 86.33 and 95.33 percent, based on Lyapunov and spectra,
	respectively. The experimental result indicated that the proposed
	method is promising. Every sample was normalized to possess unit
	energy in the preprocessing. The main problem solved in this paper
	is that it transforms the LLE into a global feature space, by which
	the features for each sample could be put in the same space, i.e.
	with the same dimension.}
}

