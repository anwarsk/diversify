@ARTICLE{mian071927,
  author = {Ajmal S. Mian and Mohammed Bennamoun and Robyn Owens},
  title = {An Efficient Multimodal 2D-3D Hybrid Approach to Automatic Face Recognition},
  journal = {IEEE Transactions on Pattern Analysis and Machine Intelligence},
  year = {2007},
  volume = {29},
  pages = {1927--1943},
  number = {11},
  month = {November},
  keywords = {Biometrics and face recognition and rejection classifier and 3D shape
	representation},
  source = {This paper discusses the problem of face recognition, particularly
	the effect of facial expression. An efficient multimodal (2D and
	3D) hybrid approach was proposed to achieve efficiency and robustness
	to facial expressions. To correct the pose of a 3D face along with
	its texture, a novel approach based on a single automatically detected
	point and Hotelling transform was proposed. In addition, by combining
	a novel 3D Spherical Face Representation (SFR) with the Scale-Invariant
	Feature Transform (SIFT) descriptor, the authors designed a rejection
	classifier to quickly eliminate a large number of candidate faces
	at an early stage for efficient recognition in case of large galleries.
	Then, a novel region-based matching approach, robust to facial expression,
	was used to verify the remaining faces. Specifically, this approach
	automatically segmented the eyes-forehead and the nose regions, two
	regions relatively less sensitive to expressions, and matched them
	separately using a modified Iterative Closest Point (ICP) algorithm.
	Information fusion from all the matching engines was performed at
	the metric level leading to higher accuracy. The proposed algorithm
	was tested on the FRGC v2.0 data. For probes with a neutral and a
	nonneutral expression, the proposed method yielded 99.74 percent
	and 98.31 percent verification rates at a 0.001 false acceptance
	rate (FAR) and identification rates of 99.02 percent and 95.37 percent,
	respectively. The results proved its superority to other algorithms
	such as the baseline PCA performance.}
}

