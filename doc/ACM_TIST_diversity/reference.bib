@CONFERENCE{Soong85,
  author = {F. K. Soong and A. E. Rosenberg and L. R. Rabiner and B. H. Juang},
  title = {A vector quantization approach to speaker recognition},
  booktitle = {Acoustics, Speech, and Signal Processing, IEEE International Conference
	on ICASSP '85},
  year = {1985},
  volume = {10},
  pages = {387--390},
  month = {March},
  annote = {This paper discussed the problem of effective representation of the
	speaker's feature for speaker recognition. Speaker-based vector quantization
	(VQ) is used to characterize the variability of short-time acoustic
	features of speakers and perform speaker recognition based on the
	minimum distance classifier. Linear prediction coefficients (LPC)
	feature vectors of dimension 9 were used to form the codebook. The
	proposed method was verified using a 100 speaker recognition experiments
	leading to a recognition of over 98 percent with a codebook of 64
	and a 10 digits long test token. Specifically, five experiments were
	designed to compare effect of codebook size and number of digits
	in the test token, repeat digits and different digits, using only
	voiced speech and different sessions. The conclusions from the results
	of five experiments can be summarized as follows: \begin{itemize}
	\item Longer test token and large codebook can improve the recognition
	rate. \item Test token with phoentically rich content yields higher
	performance than those with low phoentical content. \item If the
	unvoiced frames were used to train the codebook, these frames should
	also be used during the testing procedure. \item Due to the intra-speaker
	viriations and different recording condition, the codebook should
	be updated in order to yield high recognition rate. \end{itemize}
	}
}

